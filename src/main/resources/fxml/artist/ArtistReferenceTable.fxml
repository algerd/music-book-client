<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane id="AnchorPane" fx:id="view" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.60" 
            fx:controller="ru.javafx.musicbook.client.controller.artist.ArtistReferenceTableController">
    <children>
        <VBox fx:id="tableContainer" prefHeight="200.0" prefWidth="100.0" spacing="10.0" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="10.0" AnchorPane.rightAnchor="10.0" AnchorPane.topAnchor="10.0">
            <children>    
                <TableView fx:id="pagedTable" layoutX="169.0" layoutY="100.0" onMouseClicked="#onMouseClickTable" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <columns>
                        <TableColumn fx:id="nameReferenceColumn" prefWidth="75.0" text="Name" />
                        <TableColumn fx:id="referenceColumn" prefWidth="75.0" text="Reference" />
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                    </columnResizePolicy>
                </TableView>             
            </children> 
        </VBox>       
    </children>
</AnchorPane>
